{
  "/authorizationserver/oauth/token": "/authorization",

  "/rest/v2/:baseSiteId/languages(\\?.+)?": "/:baseSiteId-languages",
  "/rest/v2/:baseSiteId/currencies(\\?.+)?": "/:baseSiteId-currencies",
  "/rest/v2/:baseSiteId/titles(\\?.+)?": "/:baseSiteId-titles",

  "/rest/v2/:baseSiteId/cms/pages(\\?.+)&pageLabelOrId=:pageId&(.*)": "/:baseSiteId-:pageId",
  "/rest/v2/:baseSiteId/cms/pages": "/:baseSiteId-pages",
  "/rest/v2/:baseSiteId/cms/pages/:uid": "/:baseSiteId-pages/:uid",
  "/rest/v2/:baseSiteId/cms/pages?pageType=ProductPage&code=:code&(.*)": "/:baseSiteId-product-single-:code",
  "/rest/v2/:baseSiteId/cms/pages?pageType=CategoryPage&code=:code&(.*)": "/:baseSiteId-category-single-:code",
  "/rest/v2/:baseSiteId/cms/components(\\?.+)?": "/:baseSiteId-components",
  "/rest/v2/:baseSiteId/cms/components/deliveryTab(\\?.+)?": "/:baseSiteId-delivery-tab",

  "/rest/v2/:baseSiteId/products/search?query=:q(.*)": "/:baseSiteId-search?q=:q",
  "/rest/v2/:baseSiteId/products/suggestions?term=:term&max=:max(.*)": "/suggestions?q=:term&_limit=:max",
  "/rest/v2/:baseSiteId/products/:code/reviews(\\?.+)?": "/reviews?code=:code",
  "/rest/v2/:baseSiteId/products/:code?(\\?.+)?": "/:baseSiteId-products?code=:code&singular=1",
  "/rest/v2/:baseSiteId/users/:userName/carts/?(.+)curr=:currency?": "/:baseSiteId-carts?currency=:currency",
  "/rest/v2/:baseSiteId/users/:userName/carts/:cartId/entries?code=:code&qty=:quantity&(.+)": "/:baseSiteId-carts/:cartId/entries?&code=:code&quantity=:quantity",
  "/rest/v2/:baseSiteId/users/:userName": "/:baseSiteId-users?user=:userName&singular=1"
}
